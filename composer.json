{
    "name": "esensi/assembler",
    "description": "The PHP components that integrate the build tasks into Esensi",
    "keywords": ["emersonmedia", "esensi", "laravel", "build", "assembler", "artisan", "commands"],
    "authors": [
        {
            "name": "Emerson Media, LP.",
            "email": "wishlist@emersonmedia.com"
        },
        {
            "name": "Daniel LaBarge",
            "email": "daniel@emersonmedia.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "esensi/loaders": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "phpspec/phpspec": "^7.2",
        "phpunit/phpunit": "^10.0.7"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Esensi\\Assembler\\": "src/"
        }
    },
    "minimum-stability": "dev"
