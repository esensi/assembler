{
    "name": "esensi/build",
    "description": "The build components of Esensi",
    "keywords": ["emersonmedia", "esensi", "laravel", "build", "gulp"],
    "authors": [
        {
            "name": "Emerson Media, LP.",
            "email": "wishlist@emersonmedia.com"
        },
        {
            "name": "Daniel LaBarge",
            "email": "daniel@bexarcreative.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "esensi/core": "0.4.x"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "phpspec/phpspec": "2.0.*",
        "mockery/mockery": "dev-master",
        "way/generators": "1.*",
        "way/laravel-test-helpers": "dev-master"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Esensi\\Build\\": "src/"
        }
    },
    "minimum-stability": "dev"
